{
  "name": "fuego-web-client",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"node watch-api.js\" \"vite\"",
    "dev-manual": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "generate-client": "openapi-generator-cli generate -i http://backend:8888/swagger/openapi.json -g typescript-axios -o ./src/client --additional-properties=supportsES6=true,npmName=fuego-api-client,npmVersion=1.0.0 --skip-validate-spec",
    "watch-api": "node watch-api.js"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "typescript": "^5.7.3",
    "vite": "^6.0.5",
    "@openapitools/openapi-generator-cli": "^2.15.6",
    "concurrently": "^9.1.0"
  }
}